{
  "name": "tauri-plugin-ipgeo-api",
  "version": "0.1.0",
  "author": "You",
  "description": "",
  "type": "module",
  "main": "dist/index.svelte.js",
  "module": "dist/index.svelte.js",
  "types": "dist/index.svelte.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.svelte.js",
      "require": "./dist/index.svelte.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gen-bindings": "cargo test -p tauri-plugin-ipgeo && sed -i '/export type TAURI_CHANNEL<TSend> = null/d' guest-js/bindings.ts",
    "gen-typescript": "tsup guest-js/index.svelte.ts --format esm,cjs --dts --out-dir dist",
    "build": "pnpm run gen-bindings && pnpm run gen-typescript"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "~2.4.2"
  },
  "peerDependencies": {
    "svelte": "^5.46.1"
  },
  "devDependencies": {
    "svelte": "^5.46.1",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3"
  }
}
